<Control xmlns="https://spacestation14.io">
    <BoxContainer Orientation="Vertical">
        <BoxContainer Orientation="Horizontal">
            <LineEdit Name="SearchBar" HorizontalExpand="True" PlaceHolder="Search..."/>
            <Button Name="ClearButton" Text="Clear" />
        </BoxContainer>
        <ScrollContainer Name="PrototypeScrollContainer" MinSize="200 0" VerticalExpand="True">
            <PrototypeListContainer Name="PrototypeList"/>
        </ScrollContainer>
        <BoxContainer Orientation="Horizontal">
            <Button Name="EraseButton" ToggleMode="True" Text="Erase"/>
            <OptionButton Name="OverrideMenu" HorizontalExpand="True" ToolTip="Override Placement" />
        </BoxContainer>
    </BoxContainer>
</Control>